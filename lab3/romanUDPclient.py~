# -*- utf-8 -*-

from socket import *
serverName = '158.37.240.206'
serverPort = 12000
clientSocket = socket(AF_INET, SOCK_DGRAM)

message = raw_input('Input decimal number here > ')
number = int(message)
print number
clientSocket.sendto(number,(serverName, serverPort))
modifiedMessage, serverAddress = clientSocket.recvfrom(2048)
print modifiedMessage
clientSocket.close()
